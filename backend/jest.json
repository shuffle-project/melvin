{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1"
  },
  "testRegex": ".*\\.(spec|e2e-spec)\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": ["src/**/*.ts"],
  "coverageDirectory": "./coverage",
  "testEnvironment": "node"

}
